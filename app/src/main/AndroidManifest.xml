<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tritonitsolutions.layaltydemo" >
    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tritonitsolutions.loyaltydemo.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tritonitsolutions.loyaltydemo.LoginActivity"></activity>
        <activity android:name="com.tritonitsolutions.loyaltydemo.MainActivity"></activity>
        <activity android:name="com.tritonitsolutions.loyaltydemo.StoreActivity"></activity>
        <activity android:name="com.tritonitsolutions.loyaltydemo.FeedbackActivity"></activity>
        <activity android:name="com.tritonitsolutions.loyaltydemo.NewArrivalActivity"></activity>
        <activity android:name="com.tritonitsolutions.loyaltydemo.MyProfileActivity"></activity>
        <activity android:name="com.tritonitsolutions.loyaltydemo.NotificationActivity"></activity>
        <activity android:name="com.tritonitsolutions.loyaltydemo.NewUserActivity"></activity>
    </application>

</manifest>
